				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.20.1</version>
				<configuration>
					<systemProperties>
						<property>
							<name>supersede.if.properties</name>
							<value>if.development.properties</value>
						</property>
						<property>
							<name>is.admin.user</name>
							<value>admin</value>
						</property>
						<property>
							<name>is.admin.passwd</name>
							<value>$2pRSid#</value>
						</property>
					</systemProperties>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.pitest</groupId>
				<artifactId>pitest-maven</artifactId>
				<version>1.2.0</version>
				<configuration>
					<mutationEngine>descartes</mutationEngine>
					<targetTests>
						<param>eu.supersede.integration.api.adaptation.dashboard.proxies.test.*</param><!--Dspot failed-->
						<param>eu.supersede.integration.api.adaptation.proxies.test.*</param><!--Dspot failed-->
						<param>eu.supersede.integration.api.analysis.proxies.test.*</param><!--Dspot success-->
						<param>eu.supersede.integration.api.replan.controller.proxies.test.*</param><!--Dspot failed-->
						<param>eu.supersede.integration.api.replan.optimizer.proxies.test.*</param>
						<!--<param>eu.supersede.integration.api.datastore.proxies.test.*</param>-->
						<param>eu.supersede.integration.api.dm.proxies.test.DecisionMakingSystemProxyTest*</param>
						<param>eu.supersede.integration.api.fe.test.*</param>
						<param>eu.supersede.integration.api.feedback.proxies.test.*</param>
						<param>eu.supersede.integration.api.monitoring.proxies.test.*</param>
						<param>eu.supersede.integration.api.mdm.test.*</param>
						<param>eu.supersede.integration.api.monitoring.test.*</param>
						<!--<param>eu.supersede.integration.api.pubsub.test.*</param>-->
						<param>eu.supersede.integration.api.replan.controller.proxies.test.*</param>
						<param>eu.supersede.integration.api.replan.optimizer.proxies.test.*</param>
						<param>eu.supersede.integration.api.security.test.*</param>
						<!--<param>eu.supersede.integration.federation.test.*</param>-->
					</targetTests>
					<excludedClasses>
						<param>eu.supersede.integration.api.analysis.types.*</param>
						<param>eu.supersede.integration.api.datastore.integration.types.*</param>
						<param>eu.supersede.integration.api.datastore.proxies.*</param>
						<param>eu.supersede.integration.api.datastore.util.*</param>
						<param>eu.supersede.integration.api.feedback.types.*</param>
						<param>eu.supersede.integration.api.mail.proxies.*</param>
						<param>eu.supersede.integration.api.mail.types.*</param>
						<param>eu.supersede.integration.api.mdm.proxies.*</param>
						<param>eu.supersede.integration.api.mdm.types.*</param>
						<param>eu.supersede.integration.api.pubsub.*</param>
						<param>eu.supersede.integration.api.pubsub.adaptation.*</param>
						<param>eu.supersede.integration.api.pubsub.evolution.*</param>
						<param>eu.supersede.integration.api.security.*</param>
						<param>eu.supersede.integration.api.security.types.*</param>
						<param>eu.supersede.integration.api.tenants.*</param>
						<param>eu.supersede.integration.federation.*</param>
					</excludedClasses>
					<reportsDirectory>/home/stamp/Git/descartes-usecases-output/atos/supersede</reportsDirectory>
					<siteReportName>Descartes report on Atos Supersede IF UC</siteReportName>
					<outputFormats><value>HTML</value><value>JSON</value><value>METHODS</value></outputFormats>
					<mutators>
						<mutator>void</mutator>
						<mutator>null</mutator>
						<mutator>true</mutator>
						<mutator>false</mutator>
						<mutator>empty</mutator>
						<mutator>0</mutator>
						<mutator>1</mutator>
						<mutator>(byte)0</mutator>
						<mutator>(byte)1</mutator>
						<mutator>(short)1</mutator>
						<mutator>(short)2</mutator>
						<mutator>0L</mutator>
						<mutator>1L</mutator>
						<mutator>0.0</mutator>
						<mutator>1.0</mutator>
						<mutator>0.0f</mutator>
						<mutator>1.0f</mutator>
						<mutator>'\40'</mutator>
						<mutator>'A'</mutator>
						<mutator>""</mutator>
						<mutator>"A"</mutator>
					</mutators>
				</configuration>
